{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 120
        }
      },
      {
        "files": "*.tsx",
        "options": {
          "semi": true,
          "singleQuote": true,
          "jsxSingleQuote": true,
          "trailingComma": "all",
          "jsxBracketSameLine": false,
          "arrowParens": "always",
          "printWidth": 100
        }
      }
    ]
  }
  